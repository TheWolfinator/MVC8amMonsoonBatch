@model List<AdoNetExample.Models.EmployeeModel>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table>
    <tr>
        <th>Emp Id</th>
        <th>Emp Name</th>
        <th>Emp Salary</th>
    </tr>

    @foreach (var item in Model)
    {

        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>